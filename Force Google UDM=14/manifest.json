{
  "manifest_version": 3,
  "name": "Force Google UDM=14 (Content Script)",
  "version": "1.0",
  "description": "Forces Google to use udm=14 in search queries via content script.",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "*://www.google.com/search*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/search*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_title": "UDM=14 Enforcer"
  }
}
